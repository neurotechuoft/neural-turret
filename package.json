{
  "name": "NeuralTurret",
  "version": "0.1.0",
  "main": "electron.js",
  "private": true,
  "dependencies": {
    "concurrently": "^5.0.1",
    "electron": "^2.0.8",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts": "^2.1.8",
    "socket.io": "2.1.1",
    "uuid": "^3.3.2",
    "wait-on": "^3.3.0"
  },
  "scripts": {
    "prestart": "npm install",
